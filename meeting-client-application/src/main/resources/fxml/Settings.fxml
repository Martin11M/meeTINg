<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="meeting.controller.SettingsWindowController">
         <children>
            <Label layoutX="20.0" layoutY="45.0" text="Address" />
            <Label layoutX="20.0" layoutY="90.0" text="Port" />
            <TextField fx:id="addressField" layoutX="100.0" layoutY="40.0" onKeyReleased="#fieldChanged" />
            <TextField fx:id="portField" layoutX="100.0" layoutY="85.0" onKeyReleased="#fieldChanged" />
      <Button fx:id="backButton" layoutX="120.0" layoutY="135.0" mnemonicParsing="false" onAction="#backClicked" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #000000; -fx-border-width: 1.5px; -fx-border-radius: 20px;" text="Back" />
         </children>
</AnchorPane>
